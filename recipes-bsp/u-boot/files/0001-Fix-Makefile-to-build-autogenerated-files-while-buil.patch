From 25371c08de7f195c0972b85ee4d4187749c8f29e Mon Sep 17 00:00:00 2001
From: Marcin Pasinski <marcin.pasinski@cfengine.com>
Date: Thu, 9 Jun 2016 10:55:36 +0200
Subject: [PATCH] Fix Makefile to build autogenerated files while building env
 target

---
 Makefile | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/Makefile b/Makefile
index c10febd..e80017f 100644
--- a/Makefile
+++ b/Makefile
@@ -744,7 +744,7 @@ $(TIMESTAMP_FILE):
 		@LC_ALL=C date +'#define U_BOOT_TIME "%T"' >> $@.tmp
 		@cmp -s $@ $@.tmp && rm -f $@.tmp || mv -f $@.tmp $@
 
-easylogo env gdb:
+easylogo env gdb: $(VERSION_FILE) $(TIMESTAMP_FILE)
 	$(MAKE) -C tools/$@ all MTD_VERSION=${MTD_VERSION}
 gdbtools: gdb
 
-- 
